{
  "name": "diecoding/yii2-toastr",
  "description": "Simple flash toastr notifications for Yii2",
  "type": "yii2-widget",
  "keywords": [
    "yii2",
    "widget",
    "session",
    "flash",
    "toastr",
    "notification",
    "jquery"
  ],
  "homepage": "https://diecoding.com/yii2/plugins/yii2-toastr",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/die-coding/yii2-toastr/issues",
    "source": "https://github.com/die-coding/yii2-toastr"
  },
  "require": {
    "yiisoft/yii2": "^2.0.0",
    "bower-asset/toastr": "^2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "friendsofphp/php-cs-fixer": "^2.0",
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "@stable"
  },
  "autoload": {
    "psr-4": {
      "diecoding\\toastr\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "*": "auto"
    }
  },
  "authors": [
    {
      "name": "Die Coding",
      "email": "diecoding@gmail.com"
    },
    {
      "name": "Sugeng Sulistiyawan",
      "email": "sugeng.sulistiyawan@student.uns.ac.id"
    }
  ]
}
